{
  "name": "citrus-simulator",
  "version": "0.0.0",
  "private": true,
  "description": "Citrus Simulator UI",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run webapp:prod --",
    "build-watch": "concurrently 'npm run webapp:build:dev -- --watch'",
    "ci:frontend:build": "npm run webapp:build:$npm_package_config_default_environment",
    "ci:frontend:test": "npm run ci:frontend:build && npm test",
    "clean-www": "rimraf target/classes/static/simulator-ui/app/{src,target/}",
    "cleanup": "rimraf target/classes/static/simulator-ui/",
    "e2e": "playwright test",
    "e2e:open": "npm run e2e -- --open",
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --config jest.conf.js",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prettier:check": "prettier --check \"{,src/**/,webpack/,.blueprint/**/,./test/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss}\"",
    "prettier:format": "prettier --write \"{,src/**/,webpack/,.blueprint/**/,./test/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss}\"",
    "serve": "npm run start --",
    "start": "ng serve --hmr",
    "start-tls": "npm run webapp:dev-ssl",
    "pretest": "npm run lint",
    "test": "ng test --coverage --log-heap-usage -w=2",
    "test:watch": "npm run test -- --watch",
    "watch": "concurrently npm:start",
    "webapp:build": "npm run clean-www && npm run webapp:build:dev",
    "webapp:build:dev": "ng build --configuration development",
    "webapp:build:prod": "ng build --configuration production",
    "webapp:dev": "ng serve",
    "webapp:dev-ssl": "ng serve --ssl",
    "webapp:dev-verbose": "ng serve --verbose",
    "webapp:prod": "npm run clean-www && npm run webapp:build:prod",
    "webapp:test": "npm run test --"
  },
  "config": {
    "default_environment": "prod"
  },
  "dependencies": {
    "@angular/common": "19.1.3",
    "@angular/compiler": "19.1.3",
    "@angular/core": "19.1.3",
    "@angular/forms": "19.1.3",
    "@angular/localize": "19.1.3",
    "@angular/platform-browser": "19.1.3",
    "@angular/platform-browser-dynamic": "19.1.3",
    "@angular/router": "19.1.3",
    "@fortawesome/angular-fontawesome": "1.0.0",
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-brands-svg-icons": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@ng-bootstrap/ng-bootstrap": "18.0.0",
    "@ngx-translate/core": "16.0.4",
    "@ngx-translate/http-loader": "16.0.1",
    "@popperjs/core": "2.11.8",
    "bootstrap": "5.3.3",
    "bootswatch": "5.3.3",
    "dayjs": "1.11.13",
    "mdb-angular-ui-kit": "7.1.0",
    "ngx-highlightjs": "14.0.0",
    "ngx-infinite-scroll": "19.0.0",
    "prettier": "3.4.2",
    "rxjs": "7.8.1",
    "tslib": "2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "19.0.0",
    "@angular-builders/jest": "19.0.0",
    "@angular-devkit/build-angular": "19.1.4",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular/cli": "19.1.4",
    "@angular/compiler-cli": "19.1.3",
    "@angular/service-worker": "19.1.3",
    "@eslint/js": "9.19.0",
    "@playwright/test": "1.50.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.10",
    "@typescript-eslint/eslint-plugin": "8.21.0",
    "@typescript-eslint/parser": "8.21.0",
    "angular-eslint": "19.0.2",
    "browser-sync": "3.0.3",
    "browser-sync-v3-webpack-plugin": "0.1.0",
    "buffer": "6.0.3",
    "concurrently": "9.1.2",
    "copy-webpack-plugin": "12.0.2",
    "eslint": "9.19.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-webpack-plugin": "4.2.0",
    "folder-hash": "4.1.1",
    "generator-jhipster": "8.8.0",
    "globals": "15.14.0",
    "jest": "29.7.0",
    "jest-date-mock": "1.0.10",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-preset-angular": "14.5.1",
    "jest-sonar": "0.2.16",
    "merge-jsons-webpack-plugin": "2.0.1",
    "prettier-plugin-packagejson": "2.5.8",
    "rimraf": "6.0.1",
    "swagger-ui-dist": "5.18.2",
    "ts-jest": "29.2.5",
    "typescript": "<5.7",
    "typescript-eslint": "8.21.0",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-merge": "6.0.1",
    "webpack-notifier": "1.15.0"
  },
  "optionalDependencies": {
    "npm-check-updates": "17.1.14"
  },
  "engines": {
    "node": ">=20.12.1"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "overrides": {
    "webpack": "5.88.2"
  }
}
